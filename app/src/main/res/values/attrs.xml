<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ResistanceView">
        <!--背景属性-->
        <!--文字偏移量-->
        <attr name="rbvTextOffset" format="dimension"/>
        <!--文字大小-->
        <attr name="rbvTextSize" format="dimension"/>
        <attr name="rbvTextColor" format="color"/>
        <!--线条宽度-->
        <attr name="rbvBgStrokeWidth" format="dimension"/>
        <!--线条颜色-->
        <attr name="rbvBgLineColor" format="color"/>

        <!--前景属性-->
        <!--线条宽度-->
        <attr name="rvFgStrokeWidth" format="dimension"/>
        <!--线条颜色-->
        <attr name="rvFgLineColor" format="color"/>
        <!--填充色-->
        <attr name="rvFillColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="CustomToolbar">

        <attr name="showBackIcon" format="boolean"/>
    </declare-styleable>
</resources>
